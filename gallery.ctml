[html
	[head
		[title Reece Gallery]
		[meta{charset="utf-8"}])
		[link{rel="stylesheet" type="text/css"}@normal.css ])
		[link{rel="stylesheet" type="text/css"}@gallery.css ])
	]
	[body{onload="load_picture(0)"}
		[div#guide]
		[script$guideData.js ]
		[center [h1#gallery-title ]]
		[div.gallery-button{onclick="previous_picture()"} &larr;]
		[div#gallery-content [img])]
		[div.gallery-button{onclick="next_picture()"} &rarr;]
		[script$gallery.js ]
	]
]
